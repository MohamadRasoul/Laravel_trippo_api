{
    "openapi": "3.0.0",
    "info": {
        "title": "Laravel Trippo - Dashboard",
        "description": "L5 Swagger OpenApi description",
        "contact": {
            "email": "mralmaahlol@gmail.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://192.168.137.1:5000",
            "description": "Demo API Server"
        },
        {
            "url": "http://localhost:8000",
            "description": "Test Server"
        }
    ],
    "paths": {
        "/api": {},
        "/api/dashboard/city/index": {
            "get": {
                "tags": [
                    "City"
                ],
                "summary": "Get All Cities",
                "description": "",
                "operationId": "IndexCity",
                "parameters": [
                    {
                        "name": "perPage",
                        "in": "query",
                        "description": "Number of item per page",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        },
                        "example": 10
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Page number",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        },
                        "example": 1
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": "true"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "this is all cities"
                                        },
                                        "data": {
                                            "properties": {
                                                "cities": {
                                                    "$ref": "#/components/schemas/CityResource"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Error: Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerToken": []
                    }
                ]
            }
        },
        "/api/dashboard/city/store": {
            "post": {
                "tags": [
                    "City"
                ],
                "summary": "Add City",
                "description": "",
                "operationId": "StoreCity",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/StoreCityRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": "true"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "city is added success"
                                        },
                                        "data": {
                                            "properties": {
                                                "city": {
                                                    "$ref": "#/components/schemas/CityResource"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Error: Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerToken": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "LoginAdminRequest": {
                "title": "LoginAdminRequest",
                "description": "LoginAdminRequest body data",
                "required": [
                    "username",
                    "password"
                ],
                "properties": {
                    "username": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "type": "object",
                "example": {
                    "username": "admin",
                    "password": "12345678"
                }
            },
            "LoginUserRequest": {
                "title": "LoginUserRequest",
                "description": "LoginUserRequest body data",
                "required": [
                    "username",
                    "password"
                ],
                "properties": {
                    "username": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "type": "object",
                "example": {
                    "username": "mohamad_ra",
                    "password": "12345678"
                }
            },
            "RegisterAdminRequest": {
                "title": "RegisterAdminRequest",
                "description": "RegisterAdminRequest body data",
                "required": [
                    "name",
                    "username",
                    "email",
                    "password",
                    "comfirmation_password"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "username": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    },
                    "comfirmation_password": {
                        "type": "string"
                    }
                },
                "type": "object",
                "example": {
                    "name": "Mohamad Rasoul",
                    "username": "admin",
                    "email": "mralmaahlol@gmail.com",
                    "password": "12345678",
                    "password_confirmation": "12345678"
                }
            },
            "RegisterUserRequest": {
                "title": "RegisterUserRequest",
                "description": "RegisterUserRequest body data",
                "required": [
                    "first_name",
                    "last_name",
                    "username",
                    "email",
                    "password",
                    "comfirmation_password",
                    "about",
                    "gender",
                    "date_of_birthday",
                    "phone_number",
                    "latitude",
                    "longitude"
                ],
                "properties": {
                    "first_name": {
                        "type": "string"
                    },
                    "last_name": {
                        "type": "string"
                    },
                    "username": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    },
                    "comfirmation_password": {
                        "type": "string"
                    },
                    "about": {
                        "type": "string"
                    },
                    "gender": {
                        "type": "string",
                        "enum": [
                            "male",
                            "female"
                        ]
                    },
                    "date_of_birthday": {
                        "type": "string"
                    },
                    "phone_number": {
                        "type": "string"
                    },
                    "latitude": {
                        "type": "string"
                    },
                    "longitude": {
                        "type": "string"
                    }
                },
                "type": "object",
                "example": {
                    "first_name": "Mohamad",
                    "last_name": "Rasoul",
                    "username": "mohamad_ra",
                    "email": "mralmaahlol@gmail.com",
                    "password": "12345678",
                    "password_confirmation": "12345678",
                    "about": "this is my profile",
                    "gender": "male",
                    "date_of_birthday": "15-9-1998",
                    "phone_number": "0958600569",
                    "latitude": "36.1654",
                    "longitude": "37.1545"
                }
            },
            "StoreCityRequest": {
                "title": "StoreCityRequest",
                "description": "StoreCityRequest body data",
                "required": [
                    "name",
                    "description",
                    "latitude",
                    "longitude"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "latitude": {
                        "type": "string"
                    },
                    "longitude": {
                        "type": "string"
                    }
                },
                "type": "object",
                "example": {
                    "name": "Aleppo",
                    "description": "Aleppo is a city in Syria, which serves as the capital of the Aleppo Governorate, the most populous Syrian governorate with an official population of 4.6 million in 2010. Aleppo is one of the oldest continuously inhabited cities in the world; it may have been inhabited since the sixth millennium BC",
                    "latitude": "36.2021",
                    "longitude": "37.1343"
                }
            },
            "CityResource": {
                "title": "CityResource",
                "description": "CityResource body data",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "username": {
                        "type": "string"
                    }
                },
                "type": "object",
                "example": {
                    "id": 1,
                    "username": "mohamad_ra"
                }
            }
        },
        "securitySchemes": {
            "bearerToken": {
                "type": "http",
                "description": "Enter token in format (Bearer <token>)",
                "name": "Authorization",
                "in": "header",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "tags": [
        {
            "name": "City",
            "description": "API Endpoints of City"
        }
    ]
}
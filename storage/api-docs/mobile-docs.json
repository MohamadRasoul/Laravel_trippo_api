                    "date_of_birthday": {
                        "type": "string"
                    },
                    "phone_number": {
                        "type": "string"
                    },
                    "latitude": {
                        "type": "string"
                    },
                    "longitude": {
                        "type": "string"
                    }
                },
                "type": "object",
                "example": {
                    "first_name": "Mohamad",
